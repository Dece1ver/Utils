<Window x:Class="PDFTableParser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PDFTableParser"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel LastChildFill="True">
        <DockPanel DockPanel.Dock="Top" LastChildFill="True">
            <TextBlock DockPanel.Dock="Left" Text="Файл" Margin="5,5,5,0"/>
            <Button DockPanel.Dock="Right" x:Name="fileDialogButton" Content="Обзор..." Margin="5,5,3,0" Padding="3 0" Click="fileDialogButton_Click"/>
            <TextBox x:Name="filePathTB" Margin="0,5,0,0"/>
        </DockPanel>
        <TextBox x:Name="infoTB" Margin="0 5 0 0" VerticalScrollBarVisibility="Auto"/>
    </DockPanel>
</Window>
