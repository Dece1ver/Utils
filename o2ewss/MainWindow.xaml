<Window x:Class="o2ewss.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:o2ewss"
        mc:Ignorable="d"
        Title="o2ewss - Информация" Height="300" Width="450" WindowStyle="SingleBorderWindow" ResizeMode="NoResize" Loaded="Window_Loaded" ShowInTaskbar="False">
    <Grid>
        <TextBox x:Name="infoTextBox" Background="#FF252526" Foreground="{DynamicResource {x:Static SystemColors.WindowBrushKey}}" FontFamily="Consolas" FontSize="12" TextWrapping="WrapWithOverflow"/>
    </Grid>
</Window>